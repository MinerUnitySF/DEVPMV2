        <type>Date</type>
    </fields>
    <fields>
        <fullName>Lease_Term_Remaining__c</fullName>
        <externalId>false</externalId>
        <formula>(((YEAR(Lease_Expiration__c) - YEAR(TODAY()) - 1) *12) + (12 - MONTH(TODAY()) +1) + MONTH(Lease_Expiration__c))

/*( Lease_Expiration__c -  TODAY() )  / 30*/</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Lease Term Remaining</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Lease_Term__c</fullName>
        <externalId>false</externalId>
        <formula>IF( MONTH(Lease_Expiration__c)  -  MONTH(Lease_Start_Date__c)  = 0, (((YEAR(Lease_Expiration__c) - YEAR(Lease_Start_Date__c) - 1) *12) + (12 - MONTH(Lease_Start_Date__c) +1) + MONTH(Lease_Expiration__c)) - 1, (((YEAR(Lease_Expiration__c) - YEAR(Lease_Start_Date__c) - 1) *12) + (12 - MONTH(Lease_Start_Date__c) +1) + MONTH(Lease_Expiration__c))) 

/*(((YEAR(Lease_Expiration__c) - YEAR(Lease_Start_Date__c) - 1) *12) + (12 - MONTH(Lease_Start_Date__c) +1) + MONTH(Lease_Expiration__c))*/


/*(  ( Lease_Expiration__c - 30 )   -  Lease_Start_Date__c )  / 30*/</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Lease Term</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
