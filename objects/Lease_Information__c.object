        <fullName>Customer_PO__c</fullName>
        <description>Field for users to input customer PO #’s relating to leases/rentals – Only profiles needing access to this field are: FLEXX Standard User; Integration User; System Administrator; TFS Manager; TFS Data Entry</description>
        <externalId>false</externalId>
        <label>Customer PO</label>
        <length>50</length>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Expire_Date_Formula__c</fullName>
        <description>This field returns MTM (Month-to-Month) when the Expiration Date has passed and/or Lease_Expiration__c isn&apos;t filled in with a value</description>
        <externalId>false</externalId>
        <formula>IF(  
    OR(
        ISNULL( TEXT( Lease_Expiration__c) ), 
        TODAY()  &gt; Lease_Expiration__c
    ), 
    &apos;MTM&apos;,  
    TEXT(MONTH(Lease_Expiration__c)) &amp; &apos;/&apos; &amp; TEXT(DAY(Lease_Expiration__c)) &amp; &apos;/&apos; &amp; TEXT(YEAR(Lease_Expiration__c))
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Expire Date Formula</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>IP_Active__c</fullName>
        <externalId>false</externalId>
        <formula>ISPICKVAL( Installed_Product__r.SVMXC__Status__c , &apos;Active&apos;)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>IP Active</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
