    <validationRules>
        <fullName>UNITY_Agent_Cannot_Save_Manager_Statuses</fullName>
        <active>true</active>
        <description>Agent cannot save the proposal record in manager statuses</description>
        <errorConditionFormula>AND ( 
CONTAINS( $User.Id ,  UNITY_Proposal__r.UNITY_Case__r.OwnerId  ), 
OR( 
ISPICKVAL( UNITY_Proposal__r.UNITY_Proposal_Status__c , &apos;Discount - Approved by Manager&apos;), 
ISPICKVAL( UNITY_Proposal__r.UNITY_Proposal_Status__c , &apos;Discount - Rejected by Manager&apos;), 
ISPICKVAL( UNITY_Proposal__r.UNITY_Proposal_Status__c , &apos;Proposal sent for Manager Approval&apos;) 
) 
)</errorConditionFormula>
        <errorMessage>Cannot Save record with Status: Manager Approved / Rejected. Please change to the appropriate status.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>UNITY_Cancelled_Completed_Restrict</fullName>
        <active>true</active>
        <description>Restrict edit of record from completed or cancelled.</description>
        <errorConditionFormula>AND 
(
OR( 
ISPICKVAL( UNITY_Proposal__r.UNITY_Proposal_Status__c   ,&apos;Completed&apos;), 
ISPICKVAL( UNITY_Proposal__r.UNITY_Proposal_Status__c  ,&apos;Cancelled&apos;) 
),
OR( 
ISNEW(),
ISCHANGED(UNITY_Available_Quantity__c), 
ISCHANGED(UNITY_Cost__c),
ISCHANGED(UNITY_Discount__c) ,
ISCHANGED(UNITY_Type__c) , 
ISCHANGED(UNITY_Price__c) , 
ISCHANGED(UNITY_Product__c) , 
ISCHANGED(UNITY_Total_Cost__c) , 
ISCHANGED(UNITY_Total_Price__c) 
)
)</errorConditionFormula>
        <errorMessage>Record cannot be edited after it has been set to Completed or Cancelled</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>UNITY_Discount_Edit_Customer_Accept</fullName>
        <active>true</active>
        <description>Discount fields cannot be edited when customer status is Accepted</description>
        <errorConditionFormula>if( 
AND( 
ISPICKVAL(   UNITY_Proposal__r.UNITY_Status__c   ,&apos;Accepted&apos;), 
OR( ISCHANGED( UNITY_Discount__c ), ISCHANGED( UNITY_Discount_Amount__c ) ) 
) 
, true, false)</errorConditionFormula>
        <errorMessage>The Customer has Accepted the Quote, discount fields cannot be changed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>UNITY_Discount_Percent_Amount</fullName>
        <active>true</active>
        <description>Ensures that either Amount or % field is used</description>
        <errorConditionFormula>OR ( 
AND (if(UNITY_Discount__c &gt; 0,true,false), if(UNITY_Discount_Amount__c &gt; 0,true,false) ) 
)</errorConditionFormula>
        <errorMessage>Please enter value in one of the two fields: Discount % or Discount Amount</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>UNITY_Lock_Discount_Fields</fullName>
        <active>true</active>
        <description>Lock discount fields in sent to customer, approved by manager, completed, cancelled statuses</description>
        <errorConditionFormula>OR ( 
if( 
AND( 
OR (ISPICKVAL(  UNITY_Proposal__r.UNITY_Proposal_Status__c  ,&apos;Sent to Customer&apos;), ISPICKVAL(  UNITY_Proposal__r.UNITY_Proposal_Status__c  ,&apos;Completed&apos;), ISPICKVAL(  UNITY_Proposal__r.UNITY_Proposal_Status__c  ,&apos;Cancelled&apos;), ISPICKVAL(  UNITY_Proposal__r.UNITY_Proposal_Status__c  ,&apos;Returned to Agent&apos;), ISPICKVAL(  UNITY_Proposal__r.UNITY_Proposal_Status__c  ,&apos;Discount - Approved by Manager&apos;)), 
ISCHANGED( UNITY_Discount__c ) 
) 
, true, false), 
if( 
AND( 
OR (ISPICKVAL(  UNITY_Proposal__r.UNITY_Proposal_Status__c  ,&apos;Sent to Customer&apos;), ISPICKVAL(  UNITY_Proposal__r.UNITY_Proposal_Status__c  ,&apos;Completed&apos;), ISPICKVAL(  UNITY_Proposal__r.UNITY_Proposal_Status__c  ,&apos;Cancelled&apos;), ISPICKVAL(  UNITY_Proposal__r.UNITY_Proposal_Status__c  ,&apos;Returned to Agent&apos;), ISPICKVAL(  UNITY_Proposal__r.UNITY_Proposal_Status__c  ,&apos;Discount - Approved by Manager&apos;)), 
ISCHANGED( UNITY_Discount_Amount__c ) 
) 
, true, false) 
)</errorConditionFormula>
        <errorMessage>Discount fields cannot be changed in &apos;Discount - Approved by Manager&apos;, &apos;Sent to Customer&apos;, &apos;Completed&apos;, &apos;Returned to Agent&apos; or &apos;Cancelled&apos; statuses</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>UNITY_Proposal_Restrict_Customer_Edit</fullName>
        <active>true</active>
        <description>Restrict customer from editing the Proposal when it is not in &quot;Sent to Customer&quot; status</description>
        <errorConditionFormula>AND(!ISPICKVAL(  UNITY_Proposal__r.UNITY_Proposal_Status__c  , &apos;Sent to Customer&apos;) , if(CONTAINS($User.ProfileId, &apos;00e18000000Hi5J&apos;),true,false) )</errorConditionFormula>
        <errorMessage>The Quote is currently being handled by the Agent. The Quote cannot be edited at this moment. Please contact the Agent for further clarification</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>UNITY_Restrict_Agents_Skipping_Approvals</fullName>
        <active>false</active>
        <description>If discount exists and Manger Approved Discount is unchecked, the proposal cannot move into following status: Sent to Customer, Return to Agent, Proposal Completed, Proposal Amended &amp; sent to customer.</description>
        <errorConditionFormula>AND 
( 
OR 
( 
if(UNITY_Discount_Amount__c &gt; 0,true,false), 
if(UNITY_Discount__c &gt; 0,true,false), 
if( UNITY_Proposal__r.UNITY_Line_Item_Discount_Exists__c  &gt; 0,true,false), 
if( UNITY_Proposal__r.UNITY_Does_Discount_Amount_Exists__c  &gt; 0,true,false) 
) 
, 
if( UNITY_Proposal__r.UNITY_Manager_Approved_Discounts__c  = false,true,false) 
, 
OR 
( 
if(ISPICKVAL( UNITY_Proposal__r.UNITY_Proposal_Status__c , &apos;Sent to Customer&apos;),true,false), 
if(ISPICKVAL( UNITY_Proposal__r.UNITY_Proposal_Status__c , &apos;Returned to Agent&apos;),true,false), 
if(ISPICKVAL( UNITY_Proposal__r.UNITY_Proposal_Status__c , &apos;Completed&apos;),true,false) 
) 
)</errorConditionFormula>
        <errorMessage>The Quote contains discounts. Please submit the Quote for approval. Please contact the system administrator for more details.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>UNITY_Restrict_Proposal_Amended</fullName>
        <active>true</active>
        <description>Cannot move status to Proposal Amended if the Customer status is Accepted</description>
        <errorConditionFormula>AND ( OR (ISPICKVAL(  UNITY_Proposal__r.UNITY_Status__c  , &apos;Accepted&apos;) , AND (  UNITY_Proposal__r.UNITY_Manager_Approved_Discounts__c  = true, ISPICKVAL( UNITY_Proposal__r.UNITY_Status__c ,&apos;&apos;) ) ) , 

OR ( ISPICKVAL(  UNITY_Proposal__r.UNITY_Proposal_Status__c  , &apos;Proposal Amended&apos;) , ISPICKVAL(  UNITY_Proposal__r.UNITY_Proposal_Status__c  , &apos;Proposal Creation&apos;) , ISPICKVAL(  UNITY_Proposal__r.UNITY_Proposal_Status__c  , &apos;Returned to Agent&apos;)) 
)</errorConditionFormula>
        <errorMessage>The Quote cannot be amended once the customer has Accepted the Quote</errorMessage>
    </validationRules>
